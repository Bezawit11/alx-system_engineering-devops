[Unit]
Description=Gunicorn instance to serve AirBnB_clone_v4
After=network.target

[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/AirBnB_clone_v4
Environment="PATH=/home/ubuntu/AirBnB_clone_v4/myprojectenv/bin"
ExecStart=/home/ubuntu/AirBnB_clone_v4/myprojectenv/bin/gunicorn --workers 3 --bind unix:AirBnB_clone_v4.sock -m 007 2-hbnb:app

[Install]
WantedBy=multi-user.target
